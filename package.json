{
  "name": "healthy-habits-tracker",
  "version": "1.0.0",
  "private": true,
  "description": "Progressive Web App for habit tracking (vanilla-first).",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "vite build --mode analyze",
    "build:prod": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "serve": "npx serve dist -l 8100",
    "lint": "eslint src --ext .js",
    "clean": "rm -rf dist node_modules/.vite",
    "audit": "npm audit && npm run lint && npm run build",
    "build:local": "NODE_ENV=development npm run build",
    "serve:local": "cd dist && python3 -m http.server 9000",
    "build:gh": "NODE_ENV=production npm run build"
  },
  "dependencies": {
    "vite": "^5.4.19",
    "vite-plugin-pwa": "^0.19.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "keywords": [
    "pwa",
    "habits",
    "tracker",
    "health",
    "productivity",
    "javascript",
    "vite"
  ],
  "author": "Healthy Habits Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/healthy-habits-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/healthy-habits-tracker/issues"
  },
  "homepage": "https://github.com/your-org/healthy-habits-tracker#readme"
}
